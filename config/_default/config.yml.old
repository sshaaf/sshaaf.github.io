baseURL: "https://shaaf.dev"
title: "Shaaf's Blog"
paginate: 5
theme: mainroad

enableInlineShortcodes: true
enableRobotsTXT: true
buildDrafts: false
buildFuture: false
buildExpired: false
enableEmoji: true
googleAnalytics: UA-58961685-1
disqusShortname: https-shaaf-dev

menu:
      main:
        - name: Posts
          weight: 100
          identifier: posts
          url: /post/

params:
    env: production # to enable google analytics, opengraph, twitter-cards and schema.
    description: "..."
    author: Shaaf, Syed
    # author: ["Me", "You"] # multiple authors
    gravatar: "syedmshaaf@gmail.com"
    comments: false
    license: CC BY-SA 4.0
    licenseURL: 'https://creativecommons.org/licenses/by-sa/4.0'
    customCss:
      - css/blog.css
    customJs:
      - js/blog.js
    themeColor: ".theme-base-08"

    socialIcons:
        - name: github
          url: "https://github.com/sshaaf"
          icon: "fa-github"
        - name: twitter
          url: "https://twitter.com/syshaaf"
          icon: "fa-twitter"
        - name: linkedin
          url: "https://www.linkedin.com/in/shaaf"  
          icon: "fa-linkedin"

    taxonomies:
      category: categories
      series: series
      tag: tags
    markup:
      highlight:
        codeFences: true
        guessSyntax: false
        hl_Lines: ''
        lineNoStart: 1
        lineNos: false
        lineNumbersInTable: true
        noClasses: true
        style: monokai
        tabWidth: 4



